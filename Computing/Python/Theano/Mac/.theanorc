#!sh
[global]
device = gpu
force_device = False
floatX = float32
reoptimize_unpickled_function = False

[cuda]
root = /usr/local/cuda/bin

[nvcc]
# change the part after -L to <Your Anaconda Installation Folder>/lib
flags = -L/Applications/anaconda/lib

# change to /bin folder containing "clang.exe", usually "/usr/bin"
compiler_bindir = /usr/bin

fastmath = True

[blas]
# change the part after -L to <Your Anaconda Installation Folder>/lib
ldflags = -L/Applications/anaconda/lib
