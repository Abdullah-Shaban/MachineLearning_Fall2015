#!sh
[global]
device = gpu
force_device = False
floatX = float32
reoptimize_unpickled_function = False

[nvcc]
# change the part after -L to <Your Anaconda Installation Folder>/libs
flags=-LC:/Programs/Anaconda/libs

# change to <Your Visual Studio Installation Folder>/VC/bin
compiler_bindir=C:/Program Files (x86)/Microsoft Visual Studio 12.0/VC/bin

fastmath = True

[blas]
ldflags =
